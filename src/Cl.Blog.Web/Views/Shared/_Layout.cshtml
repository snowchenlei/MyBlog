<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/lib/bootstrap-3.3.7/css/bootstrap.css" rel="stylesheet" />
    @RenderSection("Style")
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <a href="/Home/Index" class="navbar-brand">博客</a>
                <a href="/Home/Index" class="navbar-brand">读书</a>
                <a href="https://github.com/snowchenlei" class="navbar-brand">开源项目</a>
            </div>
        </div>
    </div>

    <div class="container body-content" style="margin-top: 50px;">
        @RenderBody()
        <hr />
        <footer style="clear: both;">
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>


    <script src="~/Scripts/jquery-3.2.1.min.js"></script>
    <script src="~/lib/bootstrap-3.3.7/js/bootstrap.min.js"></script>
    <script src="~/Scripts/bootstrap-paginator.min.js"></script>
    <script src="~/lib/ckeditor_4.7.3_full/plugins/codesnippet/lib/highlight/highlight.pack.js"></script>
    <script src="~/Scripts/Common.js"></script>
    @RenderSection("Script")
</body>
</html>