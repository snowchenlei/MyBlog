<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Snow.Blog</title>
    <link href="~/lib/twitter-bootstrap/css/bootstrap.min.css" rel="stylesheet" asp-append-version="true" />
    @RenderSection("Style", false)
</head>
<body>
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
        <div class="container">
            <a class="navbar-brand" href="#">Navbar</a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarNav">
                <ul class="navbar-nav">
                    <li class="nav-item active">
                        <a class="nav-link" href="/Home/Index">博客 <span class="sr-only">(current)</span></a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="javascript:void(0);">读书</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="https://github.com/snowchenlei">开源项目</a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>

    <div class="container body-content" style="margin-top: 50px;">
        @RenderBody()
        <hr />
        <footer style="clear: both;">
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>
    <script src="~/lib/jquery/jquery.min.js" asp-append-version="true"></script>
    <script src="~/lib/twitter-bootstrap/js/bootstrap.bundle.js" asp-append-version="true"></script>
    @*<script src="~/lib/ckeditor_4.7.3_full/plugins/codesnippet/lib/highlight/highlight.pack.js"></script>*@
    @*<script src="~/Scripts/Common.js"></script>*@
    @RenderSection("Script", false)
</body>
</html>